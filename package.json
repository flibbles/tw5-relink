{
  "name": "relink",
  "version": "0.1.0",
  "description": "Relink - flexibility when relinking renamed tiddlers",
  "config": {
    "plugin_dir": "/usr/local/lib/node_modules/tiddlywiki/plugins/flibbles"
  },
  "scripts": {
    "test": "mocha --reporter spec",
    "build": "mkdir -p ${npm_package_config_plugin_dir} && cp -R ./plugins/flibbles/relink ${npm_package_config_plugin_dir}",
    "prebuild": "npm run clean",
    "clean": "rm -Rf ${npm_package_config_plugin_dir}/relink"
  },
  "repository": {
    "type": "git"
  },
  "author": "Flibbles",
  "license": "ISC",
  "devDependencies": {
    "tiddlywiki": "~5.1.15",
    "mocha": "^5.0.5",
    "chai": "^4.1.2"
  }
}
